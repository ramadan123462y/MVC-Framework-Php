{
  "name": "bogosoft/http-mvc",
  "description": "A model-view-controller implementation of the bogosoft/http-routing library.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bogosoft\\Http\\Mvc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bogosoft\\Http\\Mvc\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0",
    "bogosoft/http-session": "^1.0",
    "ext-json": "*",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-factory": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "guzzlehttp/psr7": "^1.6",
    "psr/container": "^1.0"
  }
}
